{
  "configurations": [
    {
      "name": "gbdk",
      "intelliSenseMode": "${default}",
      "includePath": ["${workspaceFolder}/**", "C:/gbdk/include/**"],
      "defines": ["_DEBUG", "UNICODE", "_UNICODE"],
